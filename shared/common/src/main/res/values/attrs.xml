<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GraphView">
        <attr name="textSizeVertical" format="dimension" />
        <attr name="spaceVertical" format="dimension" />
        <attr name="textSizeHorizontal" format="dimension" />
        <attr name="spaceHorizontal" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CollapsingToolbarLayout_Layout">
        <attr name="layout_collapseMode">
            <enum name="none" value="0" />
            <enum name="pin" value="1" />
            <enum name="parallax" value="2" />
        </attr>
        <attr name="layout_collapseParallaxMultiplier" format="float" />
    </declare-styleable>
    <declare-styleable name="zxing_finder">
        <attr name="zxing_possible_result_points" format="color" />
        <attr name="zxing_result_view" format="color" />
        <attr name="zxing_viewfinder_laser" format="color" />
        <attr name="zxing_viewfinder_mask" format="color" />
    </declare-styleable>
    <declare-styleable name="zxing_view">
        <attr name="zxing_scanner_layout" format="reference" />
    </declare-styleable>
    <attr name="titleTextColor" format="color" />
    <attr name="vpiTabPageIndicatorStyle" format="reference" />
    <!-- 标题栏属性 -->
    <declare-styleable name="TitleViewAttrs">
        <attr name="rightImageSrc" format="reference" />
        <attr name="leftImageSrc" format="reference" />
        <attr name="centerValue" format="string" />
        <attr name="rightValue" format="string" />
    </declare-styleable>
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="textColorOut" format="color" />
        <attr name="textColorCenter" format="color" />
        <attr name="dividerColor1" format="color" />
    </declare-styleable>

